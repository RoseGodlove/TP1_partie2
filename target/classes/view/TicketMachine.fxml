<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.controller.TicketMachineGraphicalController">
    <stylesheets>
        <URL value="@TicketMachine.css" />
    </stylesheets>
    <children>
        <Label text="Selected Route:" AnchorPane.leftAnchor="8.0" AnchorPane.topAnchor="8.0">
        </Label>

        <Text fx:id="selectedRoute" text="NONE" AnchorPane.leftAnchor="120.0" AnchorPane.topAnchor="8.0">
        </Text>

        <Button fx:id="changeToRoute1" text="Route 1" AnchorPane.leftAnchor="8.0" AnchorPane.topAnchor="40.0">
        </Button>

        <Button fx:id="changeToRoute2" text="Route 2" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="40.0">
        </Button>
      <Button layoutX="27.0" layoutY="77.0" mnemonicParsing="false" onAction="#openMachine2" text="Open machine 2" />
    </children>
</AnchorPane>
